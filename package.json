  "description": "",
  "dependencies": {
    "nanoid": "4.0.1"
  },
  "devDependencies": {
    "@types/express": "4.17.17",
    "@types/handlebars": "4.1.0",
    "@types/jest": "29.5.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "concurrently": "7.6.0",
    "dotenv": "16.0.3",
    "eslint": "8.35.0",
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.1.2",
    "nodemon": "2.0.21",
    "parcel": "2.8.3",
    "postcss": "8.4.21",
    "postcss-nested": "6.0.1",
    "prettier": "2.8.4",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "stylelint-selector-bem-pattern": "2.1.1",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "8.19.3"
  },
  "name": "middle.messenger.praktikum.yandex",
  "scripts": {
    "build": "rm -rf ./dist && rm -rf .parcel-cache && parcel build ./static/index.html",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "dev": "concurrently --kill-others \"npm run parcel:watch\" \"npm run serve-static:watch\"",
    "lint": "eslint \"**/*.{js,ts}\" && stylelint \"**/*.css\"",
    "lint-staged": "lint-staged",
    "parcel:watch": "rm -rf ./dist && rm -rf .parcel-cache && parcel ./static/index.html",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{css,js,json,md,ts}\"",
    "dev:container": "rm -rf ./dist && rm -rf .parcel-cache && docker-compose -f ./compose.dev.yml up",
    "serve-static": "ts-node ./serve-static.ts",
    "serve-static:watch": "nodemon ./serve-static.ts",
    "start": "npm run build && npm run serve-static",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "version": "1.0.0"
}
