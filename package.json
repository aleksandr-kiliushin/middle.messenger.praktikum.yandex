  "description": "",
  "dependencies": {
    "nanoid": "4.0.2"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/express": "4.17.17",
    "@types/handlebars": "4.1.0",
    "@types/jest": "29.5.0",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "concurrently": "8.0.1",
    "cypress": "12.9.0",
    "dotenv": "16.0.3",
    "eslint": "8.37.0",
    "express": "4.18.2",
    "handlebars": "4.7.7",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "nodemon": "2.0.22",
    "parcel": "2.8.3",
    "parcel-resolver-tspaths": "0.0.9",
    "postcss": "8.4.21",
    "postcss-nested": "6.0.1",
    "prettier": "2.8.7",
    "stylelint": "15.4.0",
    "stylelint-config-standard": "32.0.0",
    "stylelint-selector-bem-pattern": "2.1.1",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.3"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "8.19.3"
  },
  "name": "middle.messenger.praktikum.yandex",
  "scripts": {
    "build": "rm -rf ./dist && rm -rf .parcel-cache && parcel build ./static/index.html",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "cypress": "cypress",
    "dev": "concurrently --kill-others \"npm run parcel:watch\" \"npm run serve-static:watch\"",
    "lint": "eslint \"./src/**/*.ts\" && stylelint \"./src/**/*.css\"",
    "lint-staged": "lint-staged",
    "parcel:watch": "rm -rf ./dist && rm -rf .parcel-cache && parcel ./static/index.html",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{css,js,json,md,ts}\"",
    "dev:container": "rm -rf ./dist && rm -rf .parcel-cache && docker-compose -f ./compose.dev.yml up",
    "serve-static": "ts-node ./serve-static.ts",
    "serve-static:watch": "nodemon ./serve-static.ts",
    "start": "npm run build && npm run serve-static",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "cypress run --e2e --browser chrome"
  },
  "version": "1.0.0"
}
