{
  "author": "aleksandr-kiliushin",
  "description": "",
  "devDependencies": {
    "@anikin/parcel-transformer-handlebars": "1.0.9",
    "@types/express": "4.17.17",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "concurrently": "7.6.0",
    "eslint": "8.35.0",
    "express": "4.18.2",
    "husky": "8.0.3",
    "lint-staged": "13.1.2",
    "parcel": "2.8.3",
    "prettier": "2.8.4",
    "stylelint": "15.2.0",
    "stylelint-config-standard": "30.0.1",
    "stylelint-selector-bem-pattern": "2.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "8.19.3"
  },
  "name": "middle.messenger.praktikum.yandex",
  "scripts": {
    "build": "rm -rf ./dist && rm -rf ./.parcel-cache && parcel build ./src/pages/**/*{.hbs,.css}",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "lint": "eslint \"**/*.{js,ts}\"",
    "lint-staged": "lint-staged",
    "parcel-watch": "rm -rf ./dist && rm -rf ./.parcel-cache && parcel ./src/pages/**/*{.hbs,.css}",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{css,js,json,md,ts}\"",
    "serve-static": "ts-node ./serve-static.ts",
    "start": "concurrently --kill-others \"npm run parcel-watch\" \"npm run serve-static\""
  },
  "version": "1.0.0"
}
