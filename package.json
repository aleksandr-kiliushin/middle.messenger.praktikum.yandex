  "name": "middle.messenger.praktikum.yandex",
  "scripts": {
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "cypress": "cypress",
    "docker:build": "docker build --tag messenger ./ --file ./Dockerfile.prod",
    "docker:run": "docker run --name messenger --publish 443:3000 messenger",
    "docker:clear": "docker stop messenger && docker rm messenger && docker rmi messenger",
    "lint": "eslint \"./src/**/*.ts\" && stylelint \"./src/**/*.css\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{css,js,json,md,ts}\"",
    "dev:container": "rm -rf ./dist && docker-compose -f ./compose.dev.yml up",
    "serve-static": "ts-node ./serve-static.ts",
    "serve-static:watch": "nodemon ./serve-static.ts",
    "start": "npm run webpack:build && npm run serve-static",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "cypress run --e2e --browser chrome",
    "webpack:build": "rm -rf dist && MODE=production webpack",
    "webpack:dev": "MODE=development npx webpack serve",
    "webpack:start": "MODE=production npx webpack serve"
  },
  "version": "1.0.0"
}
